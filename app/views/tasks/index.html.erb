<% if logged_in? %>
 <h1><%= current_user.name %>のタスク一覧</h1>
  <ul>
    <% @tasks.each do |task| %>
     <li><%= link_to task.id,task %> : <%= task.status %> &gt; <%= task.content %></li>
    <% end %>
  </ul>
 <%= link_to 'タスク作成', new_task_path %>
 <%= link_to "ログアウト", logout_path, method: :delete %>
<% end %>